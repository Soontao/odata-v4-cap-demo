{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "cds run",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/@sap/cds/bin/cds",
      "args": [
        "run"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "internalConsoleOptions": "openOnSessionStart",
      "console": "internalConsole",
      "autoAttachChildProcesses": true
    }
  ]
}